{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error",
        "no-unused-expressions": "error",
        "no-console": "warn",
        "prefer-const": "error",
        "no-undef": "error"
    },
    "globals": {
        "process": "readonly"
    }
}
